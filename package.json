{
  "name": "site-react-simple-chatbot",
  "version": "0.0.1",
  "description": "React Simple Chatbot Website.",
  "scripts": {
    "start": "./node_modules/.bin/webpack-dev-server --inline --content-base build/ -d",
    "test": "./node_modules/.bin/mocha tests/**/*.spec.js --require babel-register",
    "test:watch": "./node_modules/.bin/mocha tests/**/*.spec.js --require babel-register --watch",
    "test:coverage": "nyc npm test",
    "build": "./node_modules/.bin/webpack -p"
  },
  "nyc": {
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "tests/**"
    ],
    "extension": [
      ".jsx"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/LucasBassetti/react-spa-boilerplate"
  },
  "keywords": [
    "react",
    "webpack",
    "airbnb",
    "eslint",
    "boilerplate",
    "starter",
    "open source"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LucasBassetti/react-spa-boilerplate/issues"
  },
  "homepage": "https://github.com/LucasBassetti/react-spa-boilerplate#readme",
  "devDependencies": {
    "babel-core": "^6.7.7",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "6.24.0",
    "babel-preset-react": "^6.11.1",
    "chai": "^3.5.0",
    "copy-webpack-plugin": "^4.0.1",
    "coveralls": "^2.13.1",
    "css-loader": "^0.28.4",
    "cssnano": "^3.10.0",
    "enzyme": "^2.8.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "husky": "^0.13.4",
    "ignore-styles": "^5.0.1",
    "jsdom": "^9.12.0",
    "mocha": "^3.4.2",
    "node-sass": "^4.11.0",
    "nyc": "^10.2.0",
    "postcss-import": "^10.0.0",
    "postcss-loader": "^2.0.6",
    "react-addons-test-utils": "^15.5.0",
    "redux-mock-store": "^1.2.3",
    "sass-loader": "^6.0.6",
    "sinon": "^2.3.7",
    "sinon-chai": "^2.11.0",
